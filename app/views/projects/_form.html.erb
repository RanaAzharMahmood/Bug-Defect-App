<div class="row">

  <div class="col-md-12">
    <%= form_for(@project, :html => {class: "form-horizontal", role: "form"}) do |f| %>
      
      <div class="form-group">
        <div class="control-label col-md-2">
          <%= f.label :projectname, "Name of the Project" %>
        </div>
        <div class="col-md-8">
          <%= f.text_field :projectname, class: "form-control",placeholder: "Name of Project", autofocus: true %>
        </div>
      </div>
      

      
      <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
          <%= f.submit class: "btn btn-primary btn-lg btn-xlarge" %>
        </div>
      </div>
    <% end %>
    <div class="col-md-4 col-md-offset-4 colormatch">
      [ <%= link_to "Cancel request and return to HomePage", root_path %> ]
    </div>
  </div>
</div>